 <header class="c-header">
  <div class="o-header__container">
    <%= link_to root_path do %>
      <figure>
        <%= image_tag("logo.png", alt: "Paul Brighton Web Development Logo", class: "c-brand-logo") %>
      </figure>
    <% end %>
    <div class="c-subscribe-form">
      <form action="https://twitter.us17.list-manage.com/subscribe/post?u=5e14f42c2626123e850bb543f&amp;id=5dd22b22a9" method="post" name="mc-embedded-subscribe-form" class="c-subscribe-form__container" target="_blank" novalidate>
        <input type="email" value="" placeholder="Email" name="EMAIL" class="c-subscribe-form__input" id="mce-EMAIL">
        <div style="position: absolute; left: -5000px;" aria-hidden="true"><input type="text" name="b_5e14f42c2626123e850bb543f_5dd22b22a9" tabindex="-1" value=""></div>
        <input type="submit" value="Subscribe" name="subscribe" class="c-subscribe-form__btn">
      </form>
    </div>
    <script type='text/javascript' src='//s3.amazonaws.com/downloads.mailchimp.com/js/mc-validate.js'></script>
    <script type='text/javascript'>(function($) {window.fnames = new Array(); window.ftypes = new Array();fnames[0]='EMAIL';ftypes[0]='email';}(jQuery));var $mcj = jQuery.noConflict(true);</script>
    <nav class="c-header__nav">
      <div data-behavior="myNav" class="c-overlay">
        <a href="javascript:void(0)" class="c-closebtn" onclick="closeNav()">&times;</a>
        <div class="c-overlay-content">
          <li><%= link_to "home", root_path %></li>
          <li><%= link_to "about", '#' %></li>
          <li><%= link_to "portfolio", '#' %></li>
          <li><%= link_to "contact", '#' %></li>
        </div>
      </div>
      <span class="c-hamburger-nav" onclick="openNav()">&#9776;</span>
    </nav>
    <nav class="c-main-nav">
      <ul>
        <li><%= link_to "home", root_path %></li>
        <li><%= link_to "about", '#' %></li>
        <li><%= link_to "portfolio", '#' %></li>
        <li><%= link_to "contact", '#' %></li>
      </ul>
    </nav>
  </div>
</header>